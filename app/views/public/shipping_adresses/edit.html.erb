<%= form_with model: @shipping_adress, url: public_shipping_adress_path, method: :patch do |f| %>
<tabel>
    <thead>
        <h1>配送先編集</h1>
    </thead>
    <tbody>
        <tr>
            <td><%= f.label :post_code, "郵便番号" %></td>
            <td><%= f.text_field :post_code %></td>
        </tr>
        <tr>
            <td><%= f.label :adress, "住所" %></td>
            <td><%= f.text_field :adress %></td>
        </tr>
        <tr>
            <td><%= f.label :adress_name, "宛名" %></td>
            <td><%= f.text_field :adress_name %></td>
        </tr>
        <tr>
            <%= f.submit "編集する" %>
        </tr>
    </tbody>
</tabel>
<% end %>
