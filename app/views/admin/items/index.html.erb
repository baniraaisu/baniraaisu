<h1>商品一覧</h1>

<table>
  <thead>
    <tr>
      <th>商品一覧</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @items.each do |item| %>
        <td><%= item.id %></td>
        <td><%= item.name%></td>
        <td><%= item.tex_off_price %></td>
        <td><%= item%></td>
        <td><%= item.genre.name %></td>
        <td><%= item.selling_status %></td>
      <% end %>
    </tr>
  </tbody>
</table>
