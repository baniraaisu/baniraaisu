<div class="container">
  <div class="row">

    <div class="col">
      <h1>ジャンル編集</h1>
      <%= form_with model: @genre, url: admin_genre_path, method: :patch do |f| %>
        <%= f.label :name, "ジャンル名" %>
        <%= f.text_field :name, placeholder: "genre.name" %>
        <%= f.submit "変更を保存" %>
      <% end%>
    </div>
  </div>
</div>